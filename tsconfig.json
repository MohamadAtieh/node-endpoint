{
  "compileOnSave": false,
  "buildOnSave": true,
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "dist"
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "test/**/*.ts"
  ],
  "angularCompilerOptions": {
    "skipMetadataEmit" : true
  }
}
